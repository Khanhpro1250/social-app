{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:import/typescript",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"]
  },
  "no-duplicate-imports": "error",
  "plugins": ["@typescript-eslint"]
}
